    <children>
        <ListView fx:id="listviewRequests" layoutX="470.0" layoutY="50.0" prefHeight="440.0" prefWidth="440.0" />
        <TextField fx:id="textfieldName" layoutX="128.0" layoutY="50.0" onAction="#handleTextfieldName" promptText="Name" />
        <Label layoutX="50.0" layoutY="54.0" text="Store Name:" />
        <Button fx:id="buttonSubmit" layoutX="289.0" layoutY="50.0" mnemonicParsing="false" onAction="#handleButtonSubmit" text="Submit" />
    </children>
</AnchorPane>
